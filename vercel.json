{
    "version": 2,
    "builds": [
      {
        "src": "api/run_server.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/run_server.py"
      }
    ],
    "functions": {
      "api/run_server.py": {
        "maxDuration": 60
      }
    }
}